{
  "name": "ember-vendor-split",
  "version": "1.2.0",
  "description": "Splits the vendor.js into two files: dynamic (vendor.js) and static (vendor-static.js)",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/mbutki/ember-vendor-split",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "lint:js": "eslint .",
    "test:fastboot": "mocha fastboot-test",
    "test:browser": "ember try:each",
    "test:basic": "mocha test",
    "test": "npm run test:browser && npm run test:basic"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^2.4.5",
    "broccoli-test-helper": "^1.1.0",
    "chai": "^3.5.0",
    "chai-fs": "^1.0.0",
    "chai-spies": "^0.7.1",
    "co": "^4.6.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "^3.12.0",
    "ember-cli-addon-tests": "^0.10.0",
    "ember-cli-babel": "^6.0.0",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^0.4.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "^3.12.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.2.1",
    "ember-welcome-page": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-node": "^10.0.0",
    "glob": "^7.1.2",
    "loader.js": "^4.2.3",
    "mocha": "^4.0.1"
  },
  "resolutions": {
    "**/ember-cli-babel": "^7.13.2"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "loader.js"
    ]
  },
  "dependencies": {
    "broccoli-file-creator": "^2.1.1",
    "broccoli-merge-trees": "^4.2.0"
  }
}
